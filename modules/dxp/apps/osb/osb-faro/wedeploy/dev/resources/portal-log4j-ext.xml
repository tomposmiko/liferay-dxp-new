<?xml version="1.0"?>

<Configuration strict="true">
	<Appenders>
		<Appender name="CONSOLE" type="Console">
			<Layout pattern="%-5p [%t][%c{1}:%L] %m%n" type="PatternLayout" />
		</Appender>
	</Appenders>

	<Loggers>
		<Root level="INFO">
			<AppenderRef ref="CONSOLE" />
		</Root>
	</Loggers>
</Configuration>