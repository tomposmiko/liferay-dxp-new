{
	"cpu": 8,
	"dependencies": [
		"database",
		"search"
	],
	"env": {
		"FARO_URL": "https://analytics-dev.liferay.com",
		"LCP_PROJECT_LIFERAY_CLUSTER_ENABLED": "true",
		"LIFERAY_JAVA_OPTS": "-Dorg.apache.tomcat.util.buf.UDecoder.ALLOW_ENCODED_SLASH=true",
		"OSB_API_PORT": 443,
		"OSB_API_PROTOCOL": "https",
		"OSB_API_URL": "webserver-lrkoroneiki-uat.lfr.cloud",
		"OSB_ASAH_BACKEND_URL": "https://osbasahbackend-{weDeployKey}",
		"OSB_ASAH_PUBLISHER_URL": "https://osbasahpublisher-{weDeployKey}"
	},
	"environments": {
		"infra": {
			"deploy": false
		}
	},
	"id": "liferay",
	"image": "liferaycloud/liferay-dxp:7.1-4.3.2",
	"kind": "Deployment",
	"livenessProbe": {
		"failureThreshold": 3,
		"initialDelaySeconds": 300,
		"periodSeconds": 60,
		"successThreshold": 1,
		"tcpSocket": {
			"port": 8080
		}
	},
	"memory": 8192,
	"ports": [
		{
			"external": false,
			"port": 8080
		}
	],
	"readinessProbe": {
		"failureThreshold": 3,
		"httpGet": {
			"path": "/c/portal/layout",
			"port": 8080
		},
		"initialDelaySeconds": 120,
		"periodSeconds": 15,
		"successThreshold": 3
	},
	"volumes": {
		"data": "/opt/liferay/data"
	}
}