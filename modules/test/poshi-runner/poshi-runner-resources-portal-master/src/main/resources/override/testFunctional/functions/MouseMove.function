<definition default="mouseMove">
	<command name="mouseMove">
		<execute function="WaitForSPARefresh" />

		<execute function="Poshi.MouseMove#mouseMove" />

		<execute selenium="assertJavaScriptErrors" />

		<execute selenium="assertLiferayErrors" />
	</command>

	<command name="mouseMoveAt">
		<execute function="WaitForSPARefresh" />

		<execute function="Poshi.MouseMove#mouseMoveAt" />

		<execute selenium="assertJavaScriptErrors" />

		<execute selenium="assertLiferayErrors" />
	</command>
</definition>