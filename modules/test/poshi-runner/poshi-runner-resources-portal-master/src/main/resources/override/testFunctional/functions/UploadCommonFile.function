<definition default="uploadCommonFile">
	<command name="uploadCommonFile">
		<execute function="WaitForSPARefresh" />

		<execute function="Poshi.UploadCommonFile#uploadCommonFile" />

		<execute selenium="assertJavaScriptErrors" />

		<execute selenium="assertLiferayErrors" />
	</command>

	<command name="uploadCommonFileHidden">
		<execute function="WaitForSPARefresh" />

		<execute function="Poshi.UploadCommonFile#uploadCommonFileHidden" />

		<execute selenium="assertJavaScriptErrors" />

		<execute selenium="assertLiferayErrors" />
	</command>
</definition>