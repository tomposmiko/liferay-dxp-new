<definition default="assertLocation">
	<command name="assertLocation">
		<execute function="WaitForSPARefresh" />

		<execute function="Poshi.AssertLocation#assertLocation" />

		<execute selenium="assertJavaScriptErrors" />

		<execute selenium="assertLiferayErrors" />
	</command>

	<command name="assertPartialLocation">
		<execute function="WaitForSPARefresh" />

		<execute function="Poshi.AssertLocation#assertPartialLocation" />

		<execute selenium="assertJavaScriptErrors" />

		<execute selenium="assertLiferayErrors" />
	</command>
</definition>