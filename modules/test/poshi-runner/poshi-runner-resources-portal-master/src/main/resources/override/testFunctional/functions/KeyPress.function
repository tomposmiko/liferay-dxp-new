<definition default="keyPress">
	<command name="keyDown">
		<execute function="WaitForSPARefresh" />

		<execute function="Poshi.KeyPress#keyDown" />

		<execute selenium="assertJavaScriptErrors" />

		<execute selenium="assertLiferayErrors" />
	</command>

	<command name="keyPress">
		<execute function="WaitForSPARefresh" />

		<execute function="Poshi.KeyPress#keyPress" />

		<execute selenium="assertJavaScriptErrors" />

		<execute selenium="assertLiferayErrors" />
	</command>

	<command name="keyPressNoError">
		<execute function="WaitForSPARefresh" />

		<execute function="Poshi.KeyPress#keyPressNoError" />
	</command>

	<command name="keyUp">
		<execute function="WaitForSPARefresh" />

		<execute function="Poshi.KeyPress#keyUp" />

		<execute selenium="assertJavaScriptErrors" />

		<execute selenium="assertLiferayErrors" />
	</command>
</definition>