<style>
	.has-control-menu .marketplace-navbar-wrapper {
		top: unset;
	}

	.marketplace-navbar-wrapper {
		border-bottom: 2px solid var(--neutral-8);
		left: 0;
		right: 0;
		top: 0;
		z-index: 100;
	}

	.marketplace-navbar {
		height: 64px;
		max-width: 1366px;
	}

	.marketplace-navbar .mobile-buttons {
		display: none;
	}

	.marketplace-nav .marketplace-nav-link span {
		transition: all 0.2s ease-in-out;
	}

	.marketplace-nav .marketplace-nav-link span:before,
	.marketplace-nav .marketplace-nav-link span:after {
		content: "";
		position: absolute;
		bottom: 0px;
		width: 0px;
		height: 2px;
		margin: 5px 0 0;
		transition: all 0.2s ease-in-out;
		transition-duration: 0.3s;
		opacity: 0;
		background-color: var(--action-default);
	}

	.marketplace-nav .marketplace-nav-link:hover {
		cursor: pointer;
		text-decoration: none;
	}

	.marketplace-nav .marketplace-nav-link.on-page span:before,
	.marketplace-nav .marketplace-nav-link span:before {
		left: 50%;
	}

	.marketplace-nav .marketplace-nav-link.on-page span:after,
	.marketplace-nav .marketplace-nav-link span:after {
		right: 50%;
	}

	.marketplace-nav .marketplace-nav-link.on-page span:before,
	.marketplace-nav .marketplace-nav-link.on-page span:after,
	.marketplace-nav .marketplace-nav-link:hover span:before,
	.marketplace-nav .marketplace-nav-link:hover span:after {
		width: 45%;
		opacity: 1;
	}

	@media (max-width: 587px) {
		.marketplace-navbar-wrapper {
			top: 0;
		}

		.marketplace-navbar .marketplace-logo .logo-link {
			margin-left: -44px;
		}

		.marketplace-navbar .marketplace-logo svg {
			height: 22px;
			width: 153px;
		}

		.marketplace-navbar .mobile-buttons .button-close {
			display: none;
		}

		.marketplace-navbar .marketplace-nav {
			border-bottom-left-radius: var(--border-radius);
			border-bottom-right-radius: var(--border-radius);
			left: 0;
			opacity: 0;
			position: absolute;
			top: 0;
			transition: all 0.5s cubic-bezier(0.19, 1, 0.22, 1);
			visibility: hidden;
			z-index: 2;
		}

		.marketplace-navbar.menu-open .marketplace-nav {
			opacity: 1;
			overflow: auto;
			top: calc(100% + 1px);
			transform: translateY(0);
			width: 100%;
			visibility: visible;
		}

		.marketplace-navbar.menu-open .mobile-buttons .button-close {
			display: block;
		}

		.marketplace-navbar.menu-open .mobile-buttons .button-menu {
			display: none;
		}
	}

	.layout-content #fragment-0-nhfn .portlet {
		margin-bottom: 0;
	}
</style>

