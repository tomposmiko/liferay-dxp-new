definition {

	macro assertBreadcrumbTitle {
		AssertElementPresent(
			key_breadcrumbName = "${key_value}",
			locator1 = "TestrayHeader#HEADER_BREADCRUMB_DIRECTION");
	}

	macro assertTabPageTitle {
		Click(
			key_pageTabTitle = "${pageTabTitle}",
			locator1 = "TestrayHeader#PAGE_TAB_TITLE");
	}

	macro clickBreadcrumbTitle {
		Click(
			key_breadcrumbName = "${breadcrumbName}",
			locator1 = "TestrayHeader#HEADER_BREADCRUMB_DIRECTION");
	}

}