definition {

	macro addNewCategory {
		Click(locator1 = "TestrayManage#ADD_PROJECT_ON_MODAL");

		Type(
			locator1 = "TestrayManage#NEW_OPTION_NAME",
			value1 = "${category}");

		Click(locator1 = "TestrayManage#SAVE_BUTTON");
	}

	macro addNewOptions {
		Click(locator1 = "TestrayManage#ADD_PROJECT_ON_MODAL");

		Type(
			locator1 = "TestrayManage#NEW_OPTION_NAME",
			value1 = "${options}");

		Select(
			locator1 = "TestrayManage#SELECT_CATEGORY",
			value1 = "${category}");

		Click(locator1 = "TestrayManage#SAVE_BUTTON");
	}

	macro assertName {
		AssertElementPresent(
			key_value = "${name}",
			locator1 = "TestrayManage#OPTIONS_NAME");
	}

	macro assertNameNotPresent {
		AssertElementNotPresent(
			locator1 = "TestrayManage#OPTIONS_NAME",
			value = "${name}");
	}

	macro assertTitleModal {
		AssertElementNotPresent(
			key_title = "${key_itemvalue}",
			locator1 = "TestrayManage#TITLE_MODAL_ITEM");
	}

	macro closeModalInOptions {
		Click(locator1 = "TestrayManage#CLOSE_MODAL");
	}

	macro createNewCaseTypes {
		Click(locator1 = "TestrayManage#ADD_PROJECT_ON_MODAL");

		Type(
			locator1 = "TestrayManage#NEW_OPTION_NAME",
			value1 = "${caseName}");

		Click(locator1 = "TestrayManage#SAVE_BUTTON");
	}

	macro createNewOption {
		Click(locator1 = "TestrayManage#ADD_PROJECT_ON_MODAL");

		Type(
			locator1 = "TestrayManage#NEW_OPTION_NAME",
			value1 = "${key_value}");

		Click(locator1 = "TestrayManage#SAVE_BUTTON");
	}

	macro deleteProject {
		MouseOver(
			key_value = "${valueName}",
			locator1 = "TestrayManage#OPTIONS_NAME");

		Click(locator1 = "TestrayDropdown#DROPDOWN_ITEM");

		MenuItem.clickNoError(menuItem = "Delete");
	}

	macro editNewName {
		TestrayManage.goToEditPage(valueName = "${valueName}");

		Type(
			locator1 = "TestrayManage#NEW_OPTION_NAME",
			value1 = "${valueType}");

		Click(locator1 = "TestrayManage#SAVE_BUTTON");
	}

	macro editNewOptionName {
		TestrayManage.goToEditPage(valueName = "${valueName}");

		Type(
			locator1 = "TestrayManage#NEW_OPTION_NAME",
			value1 = "${valueType}");

		Select(
			locator1 = "TestrayManage#SELECT_CATEGORY",
			value1 = "${category}");

		Click(locator1 = "TestrayManage#SAVE_BUTTON");
	}

	macro goToEditPage {
		MouseOver(
			key_value = "${valueName}",
			locator1 = "TestrayManage#OPTIONS_NAME");

		Click(locator1 = "TestrayDropdown#DROPDOWN_ITEM");

		MenuItem.clickNoError(menuItem = "Edit");
	}

	macro selectOptionsManage {
		Click(
			key_itemvalue = "${itemvalue}",
			locator1 = "TestrayDropdown#OPTIONS_SIDEBAR");
	}

}