definition {

	macro getBuildId {
		var buildId = JSONTestrayAPI._getObjectId(
			object = "builds",
			objectName = ${buildName});

		return ${buildId};
	}

	macro getCaseId {
		var caseId = JSONTestrayAPI._getObjectId(
			object = "cases",
			objectName = ${caseName});

		return ${caseId};
	}

	macro getCaseTypeId {
		var caseTypeId = JSONTestrayAPI._getObjectId(
			object = "casetypes",
			objectName = ${caseType});

		return ${caseTypeId};
	}

	macro getComponentId {
		var componentId = JSONTestrayAPI._getObjectId(
			object = "components",
			objectName = ${componentName});

		return ${componentId};
	}

	macro getFactorCategoryId {
		var factorCategoryId = JSONTestrayAPI._getObjectId(
			object = "factorcategories",
			objectName = ${categoryName});

		return ${factorCategoryId};
	}

	macro getFactorOptionsId {
		var factorOptionsId = JSONTestrayAPI._getOptionsId(optionsName = ${optionsName});

		return ${factorOptionsId};
	}

	macro getProductVersionId {
		var productVersionId = JSONTestrayAPI._getObjectId(
			object = "productversions",
			objectName = ${productVersion});

		return ${productVersionId};
	}

	macro getProjectId {
		Variables.assertDefined(parameterList = ${projectName});

		var projectId = JSONTestrayAPI._getProjectId(projectName = ${projectName});

		return ${projectId};
	}

	macro getRequirementId {
		var requirementId = JSONTestrayAPI._getObjectId(
			object = "requirements",
			objectName = ${requirementName});

		return ${requirementId};
	}

	macro getRoutineId {
		var routineId = JSONTestrayAPI._getObjectId(
			object = "routines",
			objectName = ${routineName});

		return ${routineId};
	}

	macro getSuiteId {
		var suiteId = JSONTestrayAPI._getObjectId(
			object = "suites",
			objectName = ${suiteName});

		return ${suiteId};
	}

	macro getTeamId {
		var teamId = JSONTestrayAPI._getTeamId(teamName = ${teamName});

		return ${teamId};
	}

}