definition {

	macro searchProvider {
		Click(
			dropdownLabel = "Choose an Option",
			locator1 = "ObjectField#ANY_DROPDOWN_LABEL");

		Type(
			locator1 = "ContentDashboard#SEARCH_FIELD",
			value1 = ${key_provider});
	}

	macro selectProvider {
		Click(
			dropdownLabel = "Choose an Option",
			locator1 = "ObjectField#ANY_DROPDOWN_LABEL");

		Click(
			locator1 = "FrontendDataSet#SELECT_TYPE_PROVIDER",
			value1 = ${key_type});
	}

}