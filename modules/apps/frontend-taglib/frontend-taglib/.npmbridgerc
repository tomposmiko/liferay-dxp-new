{
	"frontend-taglib": {
		"dest-mod-name-mapper": {
			"from": "(.*)\\.js$",
			"to": "bridge/frontend-taglib/$1"
		},
		"file-globs": "**/*.es.js,**/*.soy.js,!**/bridge",
		"input": "build/node/packageRunBuild/resources",
		"output": "build/node/packageRunBuild/resources/bridge/frontend-taglib",
		"src-mod-name-mapper": {
			"from": "(.*)\\.js$",
			"to": "frontend-taglib/$1"
		}
	}
}