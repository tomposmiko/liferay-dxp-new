		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="priority" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="status" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
	<class dynamic-update="true" name="com.liferay.alloy.mvc.sample.model.impl.TodoListImpl" table="AlloyMVCSample_TodoList">
		<id access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="todoListId" type="long">
			<generator class="assigned" />
		</id>
		<version access="com.liferay.portal.dao.orm.hibernate.PrivatePropertyAccessor" name="mvccVersion" type="long" />
