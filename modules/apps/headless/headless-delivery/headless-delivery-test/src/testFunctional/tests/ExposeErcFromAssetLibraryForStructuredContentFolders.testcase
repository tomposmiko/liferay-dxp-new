			TestUtils.assertPartialEquals(
				actual = "${response}",
				expected = "NOT_FOUND");
		}

		task ("And Then I receive error message in server console") {
			AssertConsoleTextPresent(value1 = "Unable to get a valid asset library with ID nonexistentId");
