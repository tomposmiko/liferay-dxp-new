	macro switchStagingStatus {
		Click(locator1 = "Button#STAGING_INDICATOR");

		DropdownMenuItem.click(menuItem = ${stagingItem});

		if (${stagingItem} == "Publish to Live") {
			SelectFrame(locator1 = "IFrame#MODAL_BODY");
		}
	}

