{
	"journal-web": {
		"dest-mod-name-mapper": {
			"from": "(.*)\\.js$",
			"to": "bridge/journal-web/$1"
		},
		"file-globs": "**/*.es.js,**/*.soy.js",
		"input": "classes/META-INF/resources",
		"output": "classes/META-INF/resources/bridge/journal-web",
		"src-mod-name-mapper": {
			"from": "(.*)\\.js$",
			"to": "journal-web/$1"
		}
	}
}