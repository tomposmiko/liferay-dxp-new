	@description = "This is a test for COMMERCE-9433. In the Product Display Page tab on Channels, it is possible to set a display page template to be shown for a specific product"
	@priority = 4
	test CanSpecificProductViewDPTSet {
		property portal.acceptance = "true";

		task ("Given a new Commerce Product type display page template with fragments created with mapped fields") {
			CommerceDisplayPageTemplates.addMappedDisplayPageTemplateEntry(
				contentType = "Commerce Product",
				displayPageName = "Test Commerce Product DPT",
				fieldsList = "Availability Status,Inventory,Name",
				fragmentName = "Heading",
				groupName = "Minium",
				siteURLKey = "minium");
		}

		task ("And admin sets the DPT for a specific product by navigating to Channels page") {
			CommerceChannels.openChannelsAdmin();

			CommerceNavigator.gotoEntry(entryName = "Minium Portal");

			CommerceEntry.gotoMenuTab(menuTab = "Product Display Pages");

			CommerceChannels.addOverrideDefaultProductDisplayPageTemplate(
				displayPageTemplateName = "Test Commerce Product DPT",
				product = "ABS Sensor");
		}

		task ("When buyer user logs in and goes to the Catalog page") {
			User.logoutPG();

			CommerceLogin.miniumLogin(
				password = "test",
				urlAppend = "web/minium",
				userEmailAddress = "buyer@liferay.com");
		}

		task ("And clicks on ABS Sensor product") {
			CommerceNavigator.searchCatalog(productName = "ABS Sensor");

			CommerceAcceleratorsInitializer.gotoMiniumProductDetailsPage(productName = "ABS Sensor");
		}

		task ("Then the mapped fields of the DPT are shown correctly for the product") {
			CommerceDisplayPageTemplates.viewMappedFragmentsText(fragmentsTextList = "available,140,ABS Sensor");
		}

		task ("And the DPT is not applied to another product") {
			Navigator.openWithAppendToBaseURL(urlAppend = "web/minium/catalog");

			CommerceNavigator.gotoProductDetails(productName = "U-Joint");

			FrontStore.assertProductDetails(
				productListPrice = "24.00",
				productName = "U-Joint",
				productSKU = "MIN55861");
		}

		task ("When admin edits the DPT product") {
			User.logoutAndLoginPG();

			CommerceChannels.openChannelsAdmin();

			CommerceNavigator.gotoEntry(entryName = "Minium Portal");

			CommerceEntry.gotoMenuTab(menuTab = "Product Display Pages");

			CommerceChannels.editOverrideProductDisplayPage(
				newProduct = "Bearings",
				product = "ABS Sensor");
		}

		task ("Then the DPT product is listed as Bearings and not ABS Sensor") {
			CommerceEntry.assertNoDisplayPageOverride(
				entryName = "ABS Sensor",
				layout = "Test Commerce Product DPT");

			CommerceEntry.assertDisplayPageOverride(
				entryName = "Bearings",
				layout = "Test Commerce Product DPT");
		}

		task ("When admin removes the DPT product") {
			CommerceEntry.deleteCommerceEntry(key_value = "Bearings");

			CommerceEntry.assertNoDisplayPageOverride(
				entryName = "Bearings",
				layout = "Test Commerce Product DPT");
		}

		task ("Then Bearings product does not show DPT mapped fields but the general product details") {
			User.logoutPG();

			CommerceLogin.miniumLogin(
				password = "test",
				urlAppend = "web/minium",
				userEmailAddress = "buyer@liferay.com");

			CommerceNavigator.searchCatalog(productName = "Bearings");

			CommerceAcceleratorsInitializer.gotoMiniumProductDetailsPage(productName = "Bearings");

			FrontStore.assertProductDetails(
				productListPrice = "70.00",
				productName = "Bearings",
				productSKU = "MIN00673");
		}
	}

	@description = "Verify that order fields values are mapped to fragments."
	@priority = 5
	test CanUseCommerceOrderFieldsWithFragments {
		property custom.properties = "jsonws.web.service.paths.excludes=${line.separator}company.security.strangers.verify=false${line.separator}feature.flag.COMMERCE-9410=true";
		property portal.acceptance = "true";

