{
	"dependencies": {
		"clay-button": "2.18.1",
		"clay-checkbox": "2.18.1",
		"clay-dropdown": "2.18.1",
		"clay-icon": "2.18.1",
		"clay-label": "2.18.1",
		"clay-radio": "2.18.1",
		"frontend-js-web": "*",
		"metal-component": "2.16.8",
		"metal-dom": "2.16.8",
		"metal-drag-drop": "3.3.1",
		"metal-position": "2.1.2",
		"metal-soy": "2.16.8",
		"moment": "^2.22.2",
		"text-mask-addons": "^3.7.1",
