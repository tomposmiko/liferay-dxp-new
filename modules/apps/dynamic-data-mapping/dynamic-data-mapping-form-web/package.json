{
	"csfConfig": {
		"path:**/*.js": {
			"js": {
				"lint": {
					"env": {
						"browser": true,
						"jest": true
					},
					"extends": "liferay",
					"globals": {
						"AlloyEditor": true
					},
					"parser": "babel-eslint",
					"parserOptions": {
						"ecmaVersion": 7
					},
					"plugins": [
						"sort-class-members"
					],
					"rules": {
						"no-unknown-property": [
							2,
							{
								"ignore": [
									"class"
								]
							}
						],
						"sort-class-members/sort-class-members": [
							2,
							{
								"groups": {
									"conventional-private-methods": [
										{
											"name": "/^_.+$/",
											"sort": "alphabetical",
											"type": "method"
										}
									],
									"methods": [
										{
											"sort": "alphabetical",
											"type": "method"
										}
									]
								},
								"order": [
									"[static-properties]",
									"[static-methods]",
									"[properties]",
									"[conventional-private-properties]",
									"constructor",
									"[methods]",
									"[conventional-private-methods]"
								]
							}
						],
						"sort-keys": [
							"error",
							"asc",
							{
								"caseSensitive": true,
								"natural": false
							}
						]
					}
				}
			}
		},
		"path:**/*.jsp": {
			"js": {
				"lint": {
					"env": {
						"browser": true,
						"jest": true,
						"mocha": true
					},
					"extends": "liferay",
					"globals": {
						"$": true,
						"AlloyEditor": true,
						"CKEDITOR": true,
						"MODULE_PATH": true,
						"ShareFormPopover": true
					}
				}
			}
		}
	},
	"dependencies": {
		"classnames": "^2.2.6",
		"clay-alert": "2.14.0",
		"clay-checkbox": "2.14.0",
		"clay-icon": "2.14.0",
		"clay-management-toolbar": "2.14.0",
		"clay-modal": "2.14.0",
		"clay-radio": "2.14.0",
		"metal-clipboard": "^2.0.1",
		"metal-debounce": "^2.0.0",
		"metal-dom": "^2.16.5",
		"metal-drag-drop": "3.3.0",
		"metal-events": "^2.16.5",
		"metal-jsx": "^2.16.5",
		"metal-position": "^2.1.2",
		"metal-state": "^2.16.5",
		"object-hash": "^1.3.0"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.3",
		"babel-eslint": "^8.2.6",
		"babel-plugin-module-resolver": "^3.1.1",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-decorators-legacy": "^1.3.5",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-metal-jsx": "^0.1.1",
		"browser-sync": "^2.24.7",
		"check-source-formatting": "^2.0.14",
		"cross-env": "^5.2.0",
		"cross-spawn": "^6.0.5",
		"eslint": "^4.10.0",
		"eslint-config-liferay": "^3.0.0",
		"jest": "^24.5.0",
		"jest-fetch-mock": "^1.6.6",
		"metal-jest-serializer": "^2.0.0",
		"metal-tools-soy": "^6.0.0",
		"opn-cli": "^3.1.0",
		"rimraf": "^2.6.2"
	},
	"jest": {
		"automock": false,
		"coverageDirectory": "build/coverage",
		"coverageThreshold": {
			"global": {
				"functions": 80,
				"lines": 80
			}
		},
		"moduleNameMapper": {
			"(.*)\\.soy$": "$1.soy.js",
			"dynamic-data-mapping-form-builder/(.*)": "<rootDir>/../dynamic-data-mapping-form-builder/src/main/resources/META-INF/resources/$1",
			"source/(.*)": "<rootDir>/src/main/resources/META-INF/resources/admin/js/$1.js"
		},
		"modulePathIgnorePatterns": [
			"/build/",
			"/classes/",
			"/__fixtures__/",
			"/__mock__/"
		],
		"setupFiles": [
			"<rootDir>/test/metal/setup/setup.js",
			"<rootDir>/test/metal/setup/setup_framework.js"
		],
		"snapshotSerializers": [
			"metal-jest-serializer"
		],
		"testMatch": [
			"<rootDir>/test/metal/js/**/*.es.js"
		],
		"testResultsProcessor": "liferay-jest-junit-reporter",
		"testURL": "http://localhost",
		"transform": {
			".*": "<rootDir>/jest-preprocessor.js"
		},
		"transformIgnorePatterns": [
			"<rootDir>.*(node_modules)(?!.*metal.*).*$"
		]
	},
	"main": "admin/js/main.es.js",
	"name": "dynamic-data-mapping-form-web",
	"private": true,
	"scripts": {
		"build": "babel --source-maps -d classes/META-INF/resources src/main/resources/META-INF/resources && liferay-npm-bundler",
		"checkFormat": "liferay-npm-scripts lint",
		"format": "liferay-npm-scripts format",
		"start": "node watch.js",
		"test:coverage:open": "opn build/coverage/lcov-report/index.html",
		"test:snapshots": "NODE_ENV=test jest --coverage -u",
		"test_DISABLED": "cross-env NODE_ENV=test jest --coverage"
	},
	"version": "2.0.0"
}