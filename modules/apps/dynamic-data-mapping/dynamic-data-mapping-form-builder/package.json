{
	"csfConfig": {
		"path:**/*.js": {
			"js": {
				"lint": {
					"env": {
						"browser": true,
						"jest": true
					},
					"extends": "liferay",
					"parser": "babel-eslint",
					"parserOptions": {
						"ecmaVersion": 7
					},
					"rules": {
						"no-unknown-property": [
							2,
							{
								"ignore": [
									"class",
									"xlink:href"
								]
							}
						]
					}
				}
			}
		}
	},
	"dependencies": {
		"classnames": "^2.2.6",
		"clay-alert": "2.14.2",
		"clay-checkbox": "2.14.2",
		"clay-dropdown": "2.14.2",
		"clay-icon": "2.14.2",
		"clay-management-toolbar": "2.14.2",
		"clay-modal": "2.14.2",
		"clay-radio": "2.14.2",
		"metal-clipboard": "2.0.1",
		"metal-debounce": "2.0.1",
		"metal-dom": "2.16.8",
		"metal-drag-drop": "3.3.1",
		"metal-events": "2.16.8",
		"metal-jsx": "2.16.8",
		"metal-position": "2.1.2",
		"metal-state": "2.16.8",
		"metal-useragent": "3.0.1",
		"object-hash": "^1.3.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/plugin-proposal-class-properties": "^7.4.0",
		"@babel/plugin-proposal-decorators": "^7.4.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.4.0",
		"@babel/preset-env": "^7.4.2",
		"babel-eslint": "^8.2.6",
		"check-source-formatting": "^2.0.14",
		"eslint": "^4.10.0",
		"eslint-config-liferay": "^2.0.8",
		"jest": "^24.5.0",
		"jest-fetch-mock": "^1.6.6",
		"metal-jest-serializer": "^2.0.0",
		"metal-tools-soy": "4.3.2",
		"opn-cli": "^3.1.0",
		"rimraf": "^2.6.2"
	},
	"jest": {
		"automock": false,
		"coverageDirectory": "build/coverage",
		"coverageThreshold": {
			"global": {
				"functions": 80,
				"lines": 80
			}
		},
		"moduleNameMapper": {
			"(.*)\\.soy$": "$1.soy.js",
			"fixtures/(.*)": "<rootDir>/test/metal/js/__fixtures__/$1.js",
			"mock/(.*)": "<rootDir>/test/metal/js/__mock__/$1.js",
			"source/(.*)": "<rootDir>/src/main/resources/META-INF/resources/js/$1.js"
		},
		"modulePathIgnorePatterns": [
			"/__fixtures__/",
			"/__mock__/",
			"/classes/"
		],
		"setupFiles": [
			"<rootDir>/test/metal/setup/setup.js"
		],
		"setupFilesAfterEnv": [
			"<rootDir>/test/metal/setup/setup_framework.js"
		],
		"snapshotSerializers": [
			"metal-jest-serializer"
		],
		"testMatch": [
			"<rootDir>/test/metal/js/**/*.js"
		],
		"testResultsProcessor": "liferay-jest-junit-reporter",
		"testURL": "http://localhost",
		"transformIgnorePatterns": [
			"<rootDir>.*(node_modules)(?!.*metal.*).*$"
		]
	},
	"name": "dynamic-data-mapping-form-builder",
	"scripts": {
		"build": "npm run build-soy && babel --source-maps -d classes/META-INF/resources src/main/resources/META-INF/resources && liferay-npm-bundler && npm run clean",
		"build-soy": "metalsoy --soyDeps \"../../../node_modules/+(clay-*|frontend-js-web)/src/**/*.soy\"",
		"build-soy-tests": "npm run build-soy && metalsoy -s \"test/**/*.soy\" -d \"test\"",
		"checkFormat": "liferay-npm-scripts lint",
		"clean": "npm run clean-soy",
		"clean-soy": "rimraf \"src/**/*.soy.js\" \"test/**/*.soy.js\"",
		"format": "liferay-npm-scripts format",
		"start": "node watch.js",
		"test:coverage:open": "opn build/coverage/lcov-report/index.html",
		"test:snapshots": "npm run build-soy-tests && cross-env NODE_ENV=test jest --coverage -u && npm run clean",
		"test_DISABLED": "npm run build-soy-tests && cross-env NODE_ENV=test jest --coverage && npm run clean"
	},
	"version": "2.0.12"
}
