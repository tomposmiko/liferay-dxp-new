				locator1 = "NavBar#NAV_ITEM_REDIRECTION");
		}
		else {
			AssertElementPresent(
				key_navItem = "404 URLs",
				locator1 = "NavBar#NAV_ITEM_REDIRECTION");
		}
	}

	macro viewDefault {
