<?xml version="1.0"?>

<taglib xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd">
	<description>Provides the Liferay Fragment component tags, prefixed with <![CDATA[<code>liferay-fragment:</code>]]>.</description>
	<tlib-version>1.0</tlib-version>
	<short-name>liferay-fragment</short-name>
	<uri>http://liferay.com/tld/fragment</uri>
	<tag>
		<name>fragment-entry-renderer</name>
		<tag-class>com.liferay.fragment.taglib.servlet.taglib.FragmentEntryRendererTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>fragmentEntryId</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
</taglib>