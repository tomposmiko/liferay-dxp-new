Bundle-Name: Liferay RSS Taglib
Bundle-SymbolicName: com.liferay.rss.taglib
Bundle-Version: 2.0.0
Export-Package: com.liferay.rss.taglib.servlet.taglib
Provide-Capability:\
	osgi.extender;\
		osgi.extender="jsp.taglib";\
		uri="http://liferay.com/tld/rss";\
		version:Version="${Bundle-Version}"
Web-ContextPath: /rss-taglib