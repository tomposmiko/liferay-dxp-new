	macro definePermissionViaJSONAPI {
		if (!(isSet(groupName))) {
			var groupName = "Global";
		}

		if (!(isSet(roleType))) {
			var roleType = "regular";
		}

		JSONPermission.editPermission(
			groupName = "${groupName}",
			method = "add-resource-permission",
			resourceAction = "${resourceAction}",
			resourceName = "${resourceName}",
			roleTitle = "${roleTitle}",
			roleType = "${roleType}"
		);
	}

