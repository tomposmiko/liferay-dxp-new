<definition>
	<command name="click">
		<execute function="Click" locator1="MenuItem#ANY_MENU_ITEM">
			<var name="key_menuItem" value="${menuItem}" />
		</execute>
	</command>

	<command name="clickNoError">
		<execute function="ClickNoError" locator1="MenuItem#ANY_MENU_ITEM">
			<var name="key_menuItem" value="${menuItem}" />
		</execute>
	</command>

	<command name="viewNotPresent">
		<execute function="AssertElementNotPresent" locator1="MenuItem#ANY_MENU_ITEM">
			<var name="key_menuItem" value="${menuItem}" />
		</execute>
	</command>

	<command name="viewPresent">
		<execute function="AssertElementPresent" locator1="MenuItem#ANY_MENU_ITEM">
			<var name="key_menuItem" value="${menuItem}" />
		</execute>
	</command>
</definition>