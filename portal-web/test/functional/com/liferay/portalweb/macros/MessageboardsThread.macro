			AssertClick(
				locator1 = "MessageBoardsAdmin#THREADS_RECYCLE_BIN_BUTTON",
				value1 = "Move to Recycle Bin");

			AssertElementPresent(locator1 = "Message#SUCCESS");
		}

		Navigator.openURL();

		ProductMenu.gotoPortlet(
			category = "Recycle Bin",
			panel = "Site Administration",
			portlet = "Recycle Bin");

		RecycleBin.emptyCP();
	}

