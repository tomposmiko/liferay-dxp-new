		);

		var key_fieldName = "name";

		AssertTextEquals(
			locator1 = "FormFields#TEXT_FIELD",
			value1 = "${kfProcessName}"
		);

		var key_fieldName = "description";

		AssertTextEquals(
			locator1 = "FormFields#TEXT_MULTILINE_FIELD",
			value1 = "${kfProcessDescription}"
