	macro addForm {
		Navigator.openURL();

		ProductMenu.gotoPortlet(
			category = "Content & Data",
			portlet = "Forms",
			site = ${site});

		AssertElementPresent(locator1 = "Message#EMPTY_INFO_TAGLIB");

		FormsAdminNavigator.gotoAddForm();

		Form.editName(formName = "Form Name");

		Form.editDescription(formDescription = "Form Description");

		Form.addTextField(fieldPositionNumber = 0);

		var fieldName = FormFields.getFieldName();

		Form.editFieldLabelMetal(fieldValue = "Text Field");

		Form.closeSidebar();

		PortletEntry.publish();

		ProductMenu.gotoPortlet(
			category = "Content & Data",
			portlet = "Forms");

		FormsAdmin.viewForm(
			formDescription = "Form Description",
			formName = "Form Name");

		ProductMenu.gotoPortlet(
			category = "Site Builder",
			portlet = "Pages");

		LexiconEntry.gotoAdd();

		if (IsElementPresent(key_menuItem = "Page", locator1 = "MenuItem#ANY_MENU_ITEM")) {
			PagesAdmin.addPage(pageName = "Form Page");
		}
		else if (IsElementPresent(key_menuItem = "Public Page", locator1 = "MenuItem#ANY_MENU_ITEM")) {
			PagesAdmin.addPublicPage(pageName = "Form Page");
		}

		Navigator.gotoPage(pageName = "Form Page");

		Portlet.addPG(
			pageName = "Form Page",
			portletName = "Form");

		FormPortletNavigator.gotoSelectForm(formName = "Form Name");

		FormPortletConfiguration.selectForm(formName = "Form Name");

		PortletEntry.save();

		Navigator.gotoPage(pageName = "Form Page");

		FormFields.viewTextField(
			fieldLabel = "Text Field",
			fieldName = ${fieldName});
	}

	macro submitFormEntry {
		Navigator.openURL();

		Navigator.gotoPage(pageName = "Form Page");

		FormFields.editText(
			fieldName = "Numeric",
			fieldValue = 1000);

		FormFields.editText(
			fieldName = "SingleLineText",
			fieldValue = "SingleLineText Test");

		FormFields.editTextMultiline(
			fieldName = "MultilineText",
			fieldValue = "MultilineText Test");

		FormFields.selectSelectOption(
			fieldName = "Select from List",
			selectOption = "Select 1");

		FormFields.selectRadioOption(
			fieldName = "SingleSelection",
			radioOption = "Option 1");

		FormFields.selectGridRadioOptionWithoutCategory(
			fieldName = "Grid",
			optionColumnNumber = 1,
			optionRowNumber = 1);

		FormViewBuilder.editDate(
			fieldName = "Date",
			fieldValue = "01/01/1000");

		FormFields.selectCheckboxOption(
			checkboxOption = "Option 2",
			fieldName = "MultipleSelection");

		FormFields.viewUploadField(
			fieldLabel = "Upload",
			fieldName = "Upload");

		ItemSelector.gotoItemSelectorViaSelectButton();

		ItemSelector.uploadFile(
			navTab = "Upload",
			uploadFileName = "Document_2.jpg");

		FormPortlet.submitSuccessfully();

		ProductMenu.gotoPortlet(
			category = "Content & Data",
			portlet = "Forms");

		FormsAdminNavigator.gotoViewEntries();

		FormsAdmin.viewFormTableEntry(
			fieldLabel = "Status",
			fieldValue = "Approved");

		FormsAdmin.viewFormTableEntry(
			fieldLabel = "Numeric",
			fieldValue = 1000);
	}

