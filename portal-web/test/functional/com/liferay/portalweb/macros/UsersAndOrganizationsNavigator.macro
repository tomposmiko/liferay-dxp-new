	macro gotoOrganizations {
		if (IsElementPresent(locator1 = "NavBar#ORGANIZATIONS")) {
			AssertClick(
				locator1 = "NavBar#ORGANIZATIONS",
				value1 = "Organizations");
		}
	}

}