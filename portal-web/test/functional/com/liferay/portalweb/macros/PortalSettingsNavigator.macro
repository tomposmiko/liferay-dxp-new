<definition>
	<command name="expandAuthenticationPanel">
		<execute macro="Panel#expandPanel">
			<var name="panel" value="Authentication" />
		</execute>
	</command>

	<command name="gotoCAS">
		<execute macro="PortalSettingsNavigator#expandAuthenticationPanel" />

		<execute function="AssertClick" locator1="NavTab#CAS" value1="CAS" />
	</command>

