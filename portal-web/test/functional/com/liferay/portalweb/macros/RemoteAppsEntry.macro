definition {

	macro addCustomElement {
		Type(
			key_text = "Name",
			locator1 = "TextInput#ANY",
			value1 = "${entryName}");

		Type(
			key_text = "HTML Element Name",
			locator1 = "TextInput#ANY",
			value1 = "${entryHTMLName}");

		Type(
			key_id = "customElementURLs",
			locator1 = "RemoteAppsEntry#URL",
			value1 = "${entryURL}");
	}

	macro addCustomElementAllFields {
		Type(
			key_text = "Name",
			locator1 = "TextInput#ANY",
			value1 = "${entryName}");

		Type(
			key_text = "HTML Element Name",
			locator1 = "TextInput#ANY",
			value1 = "${entryHTMLName}");

		Type(
			key_id = "customElementURLs",
			locator1 = "RemoteAppsEntry#URL",
			value1 = "${entryURL}");

		Type(
			key_text = "CSS URL",
			locator1 = "TextInput#ANY",
			value1 = "${entryCSSURL}");

		Type(
			locator1 = "RemoteAppsEntry#REMOTE_APPS_PROPERTIES",
			value1 = "${entryProperties}");
	}

	macro addEntry {
		Type(
			key_text = "Name",
			locator1 = "TextInput#ANY",
			value1 = "${entryName}");

		Type(
			key_id = "iFrameURL",
			locator1 = "RemoteAppsEntry#URL",
			value1 = "${entryURL}");
	}

}