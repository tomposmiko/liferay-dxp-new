<html>
<head>
<title>LanguageConfiguration</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">LanguageConfiguration</td></tr>
</thead>
<tbody>
<!--LANGUAGES-->
<tr>
	<td>LANGUAGES_CURRENT</td>
	<td>//select[contains(@id,'currentLanguageIds')]</td>
	<td></td>
</tr>
<tr>
	<td>LANGUAGES_CURRENT_SELECT</td>
	<td>//select[contains(@id,'currentLanguageIds')]/option</td>
	<td></td>
</tr>
<tr>
	<td>LANGUAGES_AVAILABLE</td>
	<td>//select[contains(@id,'availableLanguageIds')]</td>
	<td></td>
</tr>
<tr>
	<td>DISPLAY_TEMPLATE_SELECT</td>
	<td>//select[contains(@id,'displayStyle')]</td>
	<td></td>
</tr>
<!--DEPOT-->
<tr>
	<td>DEPOT_MUTED_DEFAULT_LANGUAGE</td>
	<td>//div[@class='sheet-section']//fieldset[@class='fieldset']//fieldset[contains(@class,'default-language')]//p[contains(.,'${key_language}')]</td>
	<td></td>
</tr>
<tr>
	<td>DEPOT_MUTED_TITLE</td>
	<td>//div[@class='sheet-section']//fieldset[@class='fieldset']//h4[@class='text-muted' and contains(.,'${key_titleName}')]</td>
	<td></td>
</tr>
<tr>
	<td>DEPOT_MUTED_AVAILABLE_LANGUAGES</td>
	<td>//div[@class='sheet-section']//fieldset[@class='fieldset']//fieldset[contains(@class,'available-languages')]//p[contains(.,'${key_languages}')]</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>