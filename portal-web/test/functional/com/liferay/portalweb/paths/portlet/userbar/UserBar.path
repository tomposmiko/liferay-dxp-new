<html>
<head>
<title>UserBar</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">UserBar</td></tr>
</thead>
<tbody>
<tr>
	<td>USER_AVATAR_DROPDOWN_ITEM</td>
	<td>//div[@id='clay_dropdown_portal']//div[contains(@class,'show')]//a[contains(.,'${key_dropdownItem}')]</td>
	<td></td>
</tr>
<tr>
	<td>USER_AVATAR_DROPDOWN_PORTAL_CLOSED</td>
	<td>//div[@id='clay_dropdown_portal']//div[contains(@class,'dropdown-menu')]</td>
	<td></td>
</tr>
<tr>
	<td>USER_AVATAR_DROPDOWN_PORTAL_OPEN</td>
	<td>//div[@id='clay_dropdown_portal']//div[contains(@class,'show')]</td>
	<td></td>
</tr>
<tr>
	<td>USER_AVATAR_DROPDOWN_SIGNOUT</td>
	<td>//div[@id='clay_dropdown_portal']//div[contains(@class,'show')]//a[contains(.,'Sign Out') or contains(.,'Salir')]</td>
	<td></td>
</tr>
<tr>
	<td>USER_AVATAR_IMAGE</td>
	<td>//span[@class='user-avatar-link']</td>
	<td></td>
</tr>
<tr>
	<td>USER_AVATAR_IN_CONTROL_MENU</td>
	<td>//div[contains(@class,'control-menu')]//div[@class='personal-menu-dropdown']</td>
	<td></td>
</tr>
<tr>
	<td>USER_AVATAR_IN_NAVIGATION_USER_PERSONAL_BAR_PORTLET</td>
	<td>//div[contains(@class,'portlet-user-personal-bar')]//div[@class='personal-menu-dropdown']</td>
	<td></td>
</tr>
<tr>
	<td>USER_FULL_NAME</td>
	<td>//div[@id='clay_dropdown_portal']//li[@class='dropdown-subheader']</td>
	<td></td>
</tr>
<tr>
	<td>USER_PROFILE</td>
	<td>//a[contains(.,'Profile')]</td>
	<td></td>
</tr>
<tr>
	<td>USER_DASHBOARD</td>
	<td>//a[contains(.,'Dashboard')]</td>
	<td></td>
</tr>
<tr>
	<td>USER_SIGN_OUT</td>
	<td>//a[contains(.,'Sign Out')] | //a[@href='/c/portal/logout']</td>
	<td></td>
</tr>
<tr>
	<td>USER_SIGN_IN</td>
	<td>//span[contains(@class,'sign-in')]/a/span</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>