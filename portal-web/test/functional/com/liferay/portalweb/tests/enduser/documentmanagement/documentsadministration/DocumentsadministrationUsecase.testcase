				portlet = "Documents and Media");

			DMDocument.addCP(
				dmDocumentFile = "${dmDocumentTitle}",
				dmDocumentTitle = "${dmDocumentTitle}",
				validationError = "Please enter a file with a valid extension (.doc).");
