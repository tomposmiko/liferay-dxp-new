			Segmentation.editSegment(entry = "CanEditConjunctionInConditionsSegments Test");

			AssertTextEquals(
				locator1 = "Segmentation#CRITERION_CONJUNCTION",
				value1 = "Or");
		}

		task ("Edit the condition conjunction again and save the segment") {
			Segmentation.changeCriterionConjunction(criterionConjunctionItem = "And");

			AssertTextEquals(
				locator1 = "Segmentation#CRITERION_CONJUNCTION",
				value1 = "And");

			Button.clickSave();
		}

		task ("Check if the change remains after saving the segment") {
			Segmentation.editSegment(entry = "CanEditConjunctionInConditionsSegments Test");

			AssertTextEquals(
				locator1 = "Segmentation#CRITERION_CONJUNCTION",
				value1 = "And");
		}
	}

