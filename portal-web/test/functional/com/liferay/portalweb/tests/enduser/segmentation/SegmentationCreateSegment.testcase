			Segmentation.duplicateCriteria();

			AssertTextEquals(
				locator1 = "Segmentation#CRITERION_CONJUNCTION",
				value1 = "And");

			Segmentation.changeCriterionConjunction(criterionConjunctionItem = "Or");

			Segmentation.fillProperty(
				criterionValue = "@liferay2.com",
				indexNumber = "2");

			AssertTextEquals(
