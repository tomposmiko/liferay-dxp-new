@component-name = "portal-knowledge-base"
definition {
	property custom.properties = "default.layout.template.id=1_column";
	property portal.release = "true";
	property portal.upstream = "true";
	property testray.main.component.name = "Knowledge Base";

	setUp {
		TestCase.setUpPortalInstance();

		User.firstLoginPG();
	}

	tearDown {
		var testPortalInstance = PropsUtil.get("test.portal.instance");

		if ("${testPortalInstance}" == "true") {
			PortalInstances.tearDownCP();
		}
		else {
			Workflow.tearDownSiteWorkflow();

			KBFolder.tearDownCP();

			KBArticle.tearDownCP();

			User.tearDownCP();

			Page.tearDownCP();
		}
	}

	@description = "This is a use case for LPS-68599."
	@priority = "5"
	test AddCommentInWorkflowSubmission {
		ProductMenu.gotoPortlet(
			category = "Users",
			panel = "Control Panel",
			portlet = "Users and Organizations"
		);

		User.addCP(
			userEmailAddress = "userea@liferay.com",
			userFirstName = "userfn",
			userLastName = "userln",
			userScreenName = "usersn"
		);

		ProductMenu.gotoPortlet(
			category = "Users",
			panel = "Control Panel",
			portlet = "Users and Organizations"
		);

		User.editPasswordCP(
			userEmailAddress = "userea@liferay.com",
			userScreenName = "usersn"
		);

		ProductMenu.gotoPortlet(
			category = "Users",
			panel = "Control Panel",
			portlet = "Roles"
		);

		Role.assignRegRoleCP(roleTitle = "Administrator", userScreenName = "usersn");

		Navigator.openURL();

		ProductMenu.gotoPortlet(
			category = "Configuration",
			panel = "Site Administration",
			portlet = "Workflow"
		);

		Workflow.configureWorkflow(
			workflowDefinition = "Single Approver",
			workflowResourceValue = "Knowledge Base Article"
		);

		User.logoutAndLoginPG(
			userLoginEmailAddress = "userea@liferay.com",
			userLoginFullName = "userfn userln"
		);

		ProductMenu.gotoPortlet(
			category = "Content",
			panel = "Site Administration",
			portlet = "Knowledge Base"
		);

		KBArticle.addCP(
			kbArticleContent = "Knowledge Base Article Content",
			kbArticleTitle = "Knowledge Base Article Title",
			kbWorkflowAction = "Submit for Publication"
		);

		User.logoutAndLoginPG(
			userLoginEmailAddress = "test@liferay.com",
			userLoginFullName = "Test Test"
		);

		WorkflowAsset.gotoViaNotification(
			assetType = "Knowledge Base Article",
			user = "userfn userln"
		);

		WorkflowComment.add(workflowComment = "Workflow Comment");
	}

	@priority = "3"
	test AddKBChildArticleAsDraft {
		ProductMenu.gotoPortlet(
			category = "Build",
			panel = "Site Administration",
			portlet = "Pages"
		);

		SitePages.addPublicPage(
			pageName = "Knowledge Base Page",
			sitePageType = "Widget Page"
		);

		SitePages.configurePage(
			columnName = "1 Column",
			pageName = "Knowledge Base Page",
			privacy = "Public Pages",
			sitePageType = "Widget Page"
		);

		Navigator.gotoPage(pageName = "Knowledge Base Page");

		Portlet.addPG(portletName = "Knowledge Base Display");

		Navigator.openURL();

		ProductMenu.gotoPortlet(
			category = "Content",
			panel = "Site Administration",
			portlet = "Knowledge Base"
		);

		KBArticle.addCP(
			kbArticleContent = "Knowledge Base Parent Article Content",
			kbArticleTitle = "Knowledge Base Parent Article Title"
		);

		Navigator.gotoPage(pageName = "Knowledge Base Page");

		KBArticle.addChildPGViaKBDisplay(
			kbArticleContent = "Knowledge Base Child Article Content",
			kbArticleTitle = "Knowledge Base Child Article Title",
			saveAsDraft = "true"
		);

		Pause(locator1 = "5000");

		Navigator.gotoPage(pageName = "Knowledge Base Page");

		KBArticle.viewPG(
			kbArticleContent = "Knowledge Base Parent Article Content",
			kbArticleTitle = "Knowledge Base Parent Article Title"
