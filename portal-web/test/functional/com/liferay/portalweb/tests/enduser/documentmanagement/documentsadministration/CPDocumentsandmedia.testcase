		DMNavigator.openDocumentsAndMediaAdmin(siteURLKey = "guest");

		DMNavigator.gotoDocumentCP(dmDocumentTitle = "Second edit");

		for (var versionNumber : list "1.2,1.1") {
			AssertElementPresent(
