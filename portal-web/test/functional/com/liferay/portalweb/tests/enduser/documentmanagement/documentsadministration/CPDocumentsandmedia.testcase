				locator1 = "DocumentsAndMedia#DOCUMENT_VERSION_ANY");
		}

		AssertElementNotPresent(
			key_dmDocumentVersionNumber = "1.0",
			locator1 = "DocumentsAndMedia#DOCUMENT_VERSION_ANY");
	}

