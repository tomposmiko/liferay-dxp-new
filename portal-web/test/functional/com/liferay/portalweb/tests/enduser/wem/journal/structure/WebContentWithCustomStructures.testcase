		PortletEntry.changeLocale(locale = "en-US");

		FormFields.viewTextValue(
			fieldName = "title",
			fieldValue = "English (United States) Title");

		FormFields.viewTextValueAlt(
			fieldName = "image",
			fieldValue = "English (United States) Image Description");

