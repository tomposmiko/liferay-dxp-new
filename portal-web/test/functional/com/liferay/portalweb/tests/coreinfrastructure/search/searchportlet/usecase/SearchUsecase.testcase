			<var name="panel" value="Site Administration" />
			<var name="portlet" value="Pages" />
		</execute>

		<execute macro="SitePages#addPublicPage">
			<var name="pageName" value="Search Page" />
		</execute>

		<for list="Type Facet,Search Results,Search Bar" param="portletName">
			<execute macro="Navigator#gotoPage">
				<var name="pageName" value="Search Page" />
			</execute>

			<execute macro="Portlet#addPG">
				<var name="portletName" value="${portletName}" />
			</execute>
		</for>

		<execute macro="SearchBarPortlet#configureParameterNames">
			<var name="keywordsParameterName" value="keywordparameter" />
			<var name="scopeParameterName" value="scopeparameter" />
		</execute>

		<execute macro="SearchBarPortlet#configureScope">
			<var name="searchScope" value="Let the User Choose" />
		</execute>

		<execute macro="SearchBarPortlet#selectScopeInput">
			<var name="searchScope" value="Everything" />
		</execute>

		<execute macro="SearchBarPortlet#searchPG">
			<var name="searchTerm" value="Potato" />
		</execute>

		<execute macro="SearchFacetPortlet#checkFacet">
			<var name="facetValueName" value="Blogs Entry" />
		</execute>

		<var method="selenium#getCurrentUrl()" name="searchURL" />

		<execute macro="Navigator#openURL" />

		<execute macro="Navigator#openSpecificURL">
			<var name="url" value="${searchURL}" />
		</execute>

		<execute function="AssertTextEquals" locator1="Search#SEARCH_BAR" value1="Potato" />

		<execute macro="SearchPortlet#viewSearchResultsPG">
			<var name="searchAssetTitle" value="Potato Blog" />
			<var name="searchAssetType" value="Blogs Entry" />
		</execute>

		<execute function="AssertChecked" locator1="Checkbox#ANY_CHECKBOX">
			<var name="checkboxName" value="Blogs Entry" />
		</execute>
