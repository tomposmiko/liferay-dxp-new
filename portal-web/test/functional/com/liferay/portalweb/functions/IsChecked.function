<definition default="isChecked">
	<command name="isChecked">
		<execute function="WaitForSPARefresh" />

		<execute selenium="waitForElementPresent" />

		<execute selenium="waitForVisible" />

		<execute selenium="isChecked" />
	</command>

	<command name="isCheckedNotVisible">
		<execute function="WaitForSPARefresh" />

		<execute selenium="waitForElementPresent" />

		<execute selenium="isChecked" />
	</command>
</definition>