<definition default="isNotChecked">
	<command name="isNotChecked">
		<execute function="WaitForSPARefresh" />

		<execute selenium="waitForElementPresent" />

		<execute selenium="waitForVisible" />

		<execute selenium="isNotChecked" />
	</command>

	<command name="isNotCheckedNotVisible">
		<execute function="WaitForSPARefresh" />

		<execute selenium="waitForElementPresent" />

		<execute selenium="isNotChecked" />
	</command>
</definition>