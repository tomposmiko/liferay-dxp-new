<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.liferay.counter.kernel.model.Counter" />
	<class name="com.liferay.counter.model.impl.CounterImpl" table="Counter">
		<id access="com.liferay.portal.dao.orm.hibernate.MethodPropertyAccessor" name="name" type="java.lang.String">
			<generator class="assigned" />
		</id>
		<property access="com.liferay.portal.dao.orm.hibernate.MethodPropertyAccessor" name="currentId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
</hibernate-mapping>